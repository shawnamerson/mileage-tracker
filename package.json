{
  "name": "mileage-tracker",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:increment": "node ./scripts/increment-build.js --platform=all",
    "build:ios": "node ./scripts/increment-build.js --platform=ios && git add app.json && eas build --platform ios",
    "build:ios:prod": "node ./scripts/increment-build.js --platform=ios && git add app.json && eas build --platform ios --profile production",
    "build:android": "node ./scripts/increment-build.js --platform=android && git add app.json && eas build --platform android",
    "build:android:prod": "node ./scripts/increment-build.js --platform=android && git add app.json && eas build --platform android --profile production",
    "build:all": "node ./scripts/increment-build.js --platform=all && git add app.json && eas build --platform all"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.75.1",
    "expo": "54.0.20",
    "expo-apple-authentication": "^8.0.7",
    "expo-constants": "~18.0.9",
    "expo-document-picker": "^14.0.7",
    "expo-file-system": "^19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-location": "^19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.13",
    "expo-secure-store": "^15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-task-manager": "~14.0.8",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^2.0.0",
    "react-native-iap": "^14.4.23",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
